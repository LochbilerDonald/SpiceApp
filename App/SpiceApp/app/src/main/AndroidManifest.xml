<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spiceapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RegisterPage"></activity>
        <activity android:name=".LoginPage" />
        <activity
            android:name=".SocialPage"
            android:label="@string/title_activity_social_page"
            android:parentActivityName=".HomePage$HomePageActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SpiceItUp"
            android:parentActivityName=".HomePage$HomePageActivity" />
        <activity android:name=".HomePage" />
        <activity android:name=".HomePage$HomePageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>